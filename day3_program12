PRODUCT OF LIST

#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    int n, i;
    float value, product = 1.0;

    scanf("%d", &n);

   
    for(i = 0; i < n; i++) {
        scanf("%f", &value);
        product *= value;
    }

  
    printf("The Product of list value is %.2f\n", product);

    return 0;
}
