#include <stdbool.h>

int main() {
    
    float a;
    int b;
    int c;
    int d;
    scanf("%f%d%d%d",&a,&b,&c,&d);
    int e,f,g,h;
    e=floor(a);
    f=ceil(a);
    g=sqrt(b);
    h=pow(c,d);
    printf("%d\n",e);
    printf("%d\n",f);
    printf("%d\n",g);
    printf("%d",h);
