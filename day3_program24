Count distinct elements:

#include <math.h>
#include <stdio.h>
#include <string.h>
#include <stdlib.h>
#include <assert.h>
#include <limits.h>
#include <stdbool.h>

int main() {
    
    int n;
    scanf("%d", &n);

    int a[100], count = 0;

    for (int i = 0; i < n; i++)
        scanf("%d", &a[i]);

    for (int i = 0; i < n; i++) {
        int isNew = 1;
        for (int j = 0; j < i; j++) {
            if (a[i] == a[j]) {
                isNew = 0;
                break;
            }
        }
        if (isNew)
            count++;
    }

    if (count == 1)
        printf("There are 1 distinct element in the array.\n");
    else
        printf("There are %d distinct elements in the array.\n", count);

    return 0;
}
