Same or Not:

#include <math.h>
#include <stdio.h>
#include <string.h>
#include <stdlib.h>
#include <assert.h>
#include <limits.h>
#include <stdbool.h>


int main() {
    int size, i;
    int even = 0, odd = 0;

    scanf("%d", &size);
    int arr[size];

    for (i = 0; i < size; i++) {
        scanf("%d", &arr[i]);

        if (arr[i] % 2 == 0)
            even++;
        else
            odd++;
    }

    if (even == size)
        printf("Even\n");
    else if (odd == size)
        printf("Odd\n");
    else
        printf("Mixed\n");

    return 0;
}
