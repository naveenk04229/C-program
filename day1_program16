Wind chill factor:

#include <math.h>
#include <stdio.h>
#include <string.h>
#include <stdlib.h>
#include <assert.h>
#include <limits.h>
#include <stdbool.h>

int main() {
    
    int t;
    int v;
    float wcf;
    scanf("%d%d",&t,&v);
    wcf = 35.74 + 0.6215*t + (0.4275*t - 35.75)*pow(v,0.16);
    printf("%.2f",wcf);
   
    return 0;
}
